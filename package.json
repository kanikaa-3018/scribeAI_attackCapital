{
  "name": "attack-capital-scribeai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm:dev:next\" \"npm:dev:server\"",
    "dev:next": "next dev",
    "build": "next build && tsc -p tsconfig.server.json",
    "start": "concurrently \"next start\" \"node ./dist/server/index.js\"",
    "build:server": "tsc -p tsconfig.server.json",
    "watch:server": "tsc -w -p tsconfig.server.json",
    "dev:server": "concurrently \"npm:watch:server\" \"npx nodemon --watch dist/server --exec node ./dist/server/index.js\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "@prisma/client": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "next": "14",
    "node-fetch": "^3.3.2",
    "node-wav": "^0.0.2",
    "pg": "^8.11.0",
    "prisma": "^5.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "socket.io": "^4.7.1",
    "socket.io-client": "^4.7.1",
    "tailwindcss": "^4.1.17",
    "ts-node": "^10.9.1",
    "typescript": "5.4.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/express": "^5.0.5",
    "@types/react": "19.2.6",
    "autoprefixer": "^10.4.22",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.6"
  }
}
